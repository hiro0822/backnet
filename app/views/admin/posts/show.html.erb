<h1>投稿詳細</h1>

<table class="table table-striped">
  <tr>

    <td><%= @post.title %></td>
    <td><%= @post.body %></td>
    
    <td><%=link_to "削除",admin_post_path(@post.id),class: "btn btn-danger", method: :delete%></td>
  </tr>

</table>

<table>
  <%=render"admin/comments/comments",posts: @post,user: @user%>
</table>

